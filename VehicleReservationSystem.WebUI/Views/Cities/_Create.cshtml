@using VehicleReservationSystem.WebUI.Models
@model CityModel

@using (Html.BeginForm("create", "cities", FormMethod.Post, new { id = "ceatedeptform", enctype = "multipart/form-data" }))
{
    <div class="form-group">
        <div class="input-group has-feedback">
            <span class="input-group-addon"><span class="glyphicon glyphicon-edit"></span></span>
            @Html.TextBoxFor(m => m.Name, new { @class = "form-control", placeholder = "Enter City Name" })
            <span class="form-control-feedback"></span>
        </div>
    </div>
            <div class="form-group">
                <button type="submit" class="btn btn-primary"><span class="glyphicon glyphicon-log-in"></span>  Save</button>
            </div>
}



